    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^7.0.4",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-jasmine2": "28.1.1",
    "jest-marbles": "^3.0.2",
    "jest-preset-angular": "12.2.6",
    "lint-staged": "^12.3.7",
    "ng-packagr": "15.0.3",
