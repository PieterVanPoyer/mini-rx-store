    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@jscutlery/semver": "^1.0.0",
    "@nrwl/cli": "15.2.0",
    "@nrwl/cypress": "15.2.0",
    "@nrwl/eslint-plugin-nx": "15.2.0",
    "@nrwl/jest": "15.2.0",
    "@nrwl/js": "15.2.0",
    "@nrwl/linter": "15.2.0",
    "@nrwl/nx-cloud": "15.3.5",
    "@nrwl/web": "15.2.0",
    "@nrwl/workspace": "15.2.0",
