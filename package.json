    "@angular/router": "15.0.4",
    "@nrwl/angular": "15.2.0",
    "angular-in-memory-web-api": "^0.15.0",
    "bootstrap": "^4.4.1",
    "bootstrap-icons": "^1.4.0",
    "lodash-es": "^4.17.21",
    "ngx-toastr": "^15.2.2",
    "rxjs": "~7.5.0",
    "ts-action": "^11.0.0",
    "ts-action-operators": "^9.1.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.5",
